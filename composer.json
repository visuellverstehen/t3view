{
    "name": "visuellverstehen/t3view",
    "type": "typo3-cms-extension",
    "description": "An extension that outputs system information as JSON",
    "homepage": "https://github.com/visuellverstehen/t3view",
    "license": [],
    "authors": [
        {
            "name": "visuellverstehen",
            "email": "kontakt@visuellverstehen.de",
            "homepage": "https://www.visuellverstehen.de"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    ],
    "require": {
        "typo3/cms-core": ">=6.2.0 <9.0.0",
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "VV\\T3view\\": "Classes/"
        }
    },
    "scripts": {
        "extension-release": [
            "rm .gitignore",
            "rm .travis.yml",
            "rm -rf .git",
            "rm -rf typo3",
            "rm -rf vendor"
        ]
    }
}
